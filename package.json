  "proxy": "http://localhost:3001/",
  "scripts": {
    "clean": "rimraf build",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch:css": "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p watch:css start:js start:server:dev",
    "start:server:dev": "NODE_ENV=development nodemon server/index.js --watch server",
    "start:server:prod": "API_URL=http://localhost:3001 NODE_ENV=production node server/index.js",
    "build:js": "react-scripts build",
    "build": "API_URL=http://localhost:3001 npm run clean && npm-run-all -p build:css build:js",
    "serve": "npm run build && npm run start:server:prod",
    "test": "API_URL=http://localhost:3001 react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "storybook-addon-intl": "^2.1.1",
    "storybook-addon-jsx": "^4.1.1",
    "storybook-router": "^0.2.9",
    "supertest": "^3.0.0"
  }
